{
  "name": "@privyid/persona",
  "version": "0.4.0",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs",
      "types": "./dist/module.d.ts"
    },
    "./core": {
      "import": "./dist/core/index.mjs",
      "types": "./dist/core/index.d.ts"
    },
    "./directive": {
      "import": "./dist/directive/index.mjs",
      "types": "./dist/directive/index.d.ts"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "yarn build && yarn cleanup",
    "build": "nuxt-module-build",
    "cleanup": "del-cli dist/**/*.spec.{d.ts,mjs} dist/**/__mocks__ dist/**/*.md",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground"
  },
  "dependencies": {
    "@carbon/icons-vue": "10.49.1",
    "@nuxt/kit": "3.0.0",
    "@nuxtjs/tailwindcss": "5.3.5",
    "@popperjs/core": "2.11.6",
    "@privyid/tailwind-preset": "workspace:^",
    "@vueuse/core": "8.9.4",
    "@zxing/browser": "0.1.1",
    "@zxing/library": "0.19.1",
    "buffer": "6.0.3",
    "chart.js": "3.9.1",
    "date-fns": "2.29.3",
    "defu": "6.1.1",
    "fuse.js": "6.6.2",
    "interactjs": "1.10.17",
    "lodash-es": "4.17.21",
    "postcss-custom-properties": "12.1.8",
    "tabbable": "6.0.1",
    "tailwindcss": "3.2.4",
    "vue-demi": "0.13.11",
    "vuedraggable": "next",
    "webfontloader": "1.6.28"
  },
  "devDependencies": {
    "@nuxt/module-builder": "latest",
    "@nuxt/schema": "3.0.0",
    "del-cli": "5.0.0",
    "nuxt": "3.0.0"
  },
  "installConfig": {
    "hoistingLimits": "workspaces"
  }
}
